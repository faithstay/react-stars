{
  "name": "@jongleberry/react-stars",
  "version": "2.3.2",
  "description": "Simple star rating component for your React projects",
  "main": "dist/react-stars.js",
  "repository": "jonathanong/react-stars-1",
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src -s inline --out-dir dist",
    "dev": "babel src --out-dir dist --watch",
    "build-example": "webpack -p --config=wepack.production.config.js",
    "dev-example": "webpack-dev-server . --hot --inline",
    "postpublish": "greenkeeper-postpublish"
  },
  "keywords": [
    "star",
    "rating",
    "react",
    "star",
    "rating",
    "component",
    "raty"
  ],
  "author": "Oleg Berman",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-cli": "^6.10.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "greenkeeper-postpublish": "^1.0.1",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^15.1.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "babel-runtime": "^6.9.2"
  },
  "files": [
    "dist"
  ]
}
